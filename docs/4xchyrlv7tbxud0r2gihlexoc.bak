				"code" : "| engine bubblePipeline consumeStage visibleStage viewStage\n  regionId bubbleId region bubble\n  e1 e2 n1 n2\n  view1 view2 view3 |\n\nengine := CESimEngine new.\n\nconsumeStage := CESimStage new\n    stageId: #consumeCommands;\n    runner: CESimConsumeCommandsStageRunner new;\n    yourself.\n\nvisibleStage := CESimStage new\n    stageId: #collectVisible;\n    runner: CESimCollectVisibleSnapshotsStageRunner new;\n    yourself.\n\nviewStage := CESimStage new\n    stageId: #buildLocalView;\n    runner: CESimBuildLocalViewStageRunner new;\n    yourself.\n\nbubblePipeline := CESimPipeline new\n    pipelineId: #bubble;\n    stages: { consumeStage . visibleStage . viewStage };\n    yourself.\n\nengine bubblePipeline: bubblePipeline.\n\nregionId := CERegionId fromInteger: 1.\nbubbleId := CEBubbleId fromInteger: 1.\nregion := engine ensureRegion: regionId.\nbubble := engine ensureBubble: bubbleId.\n\n\"Add two notables\"\ne1 := CEEntityId fromInteger: 101.\ne2 := CEEntityId fromInteger: 102.\n\nn1 := CENotable id: e1 definitionKey: #wolf transform: (Dictionary new at: #x put: 0; at: #y put: 0; at: #z put: 0; yourself)\n                    state: (Dictionary new at: #health put: 1.0; yourself).\nn2 := CENotable id: e2 definitionKey: #bear transform: (Dictionary new at: #x put: 10; at: #y put: 0; at: #z put: 0; yourself)\n                    state: (Dictionary new at: #health put: 0.8; yourself).\nn1 := CENotable\n    id: e1\n    definitionKey: #wolf\n    transform: (Dictionary new at: #x put: 0; at: #y put: 0; at: #z put: 0; yourself)\n    state: (Dictionary new at: #health put: 1.0; yourself).\n\nn2 := CENotable\n    id: e2\n    definitionKey: #bear\n    transform: (Dictionary new at: #x put: 10; at: #y put: 0; at: #z put: 0; yourself)\n    state: (Dictionary new at: #health put: 0.8; yourself).\r                    \nregion addNotable: n1.\nregion addNotable: n2.\n\n\"Tick 1: should spawn 2\"\nview1 := engine advanceBubbleTickFor: bubbleId.\n(view1 spawns size = 2) ifFalse: [ self error: 'Expected 2 spawns on first view' ].\n(view1 updates isEmpty) ifFalse: [ self error: 'Expected 0 updates on first view' ].\n(view1 despawns isEmpty) ifFalse: [ self error: 'Expected 0 despawns on first view' ].\n\n\"Mutate a notable (simulate movement) and tick 2: should be 2 updates, 0 spawns/despawns\"\nn1 transform at: #x put: 1.\nview2 := engine advanceBubbleTickFor: bubbleId.\n(view2 spawns isEmpty) ifFalse: [ self error: 'Expected 0 spawns on second view' ].\n(view2 updates size = 2) ifFalse: [ self error: 'Expected 2 updates on second view' ].\n(view2 despawns isEmpty) ifFalse: [ self error: 'Expected 0 despawns on second view' ].\n\n\"Remove one notable and tick 3: should despawn 1\"\nregion removeNotableId: e2.\nview3 := engine advanceBubbleTickFor: bubbleId.\n(view3 spawns isEmpty) ifFalse: [ self error: 'Expected 0 spawns on third view' ].\n(view3 updates size = 1) ifFalse: [ self error: 'Expected 1 update on third view' ].\n(view3 despawns size = 1) ifFalse: [ self error: 'Expected 1 despawn on third view' ].\n\n{ #view1 -> view1. #view2 -> view2. #view3 -> view3 }"
